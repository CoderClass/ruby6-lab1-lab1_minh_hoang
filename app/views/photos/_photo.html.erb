<div class="card">
  <%= image_tag(photo.url, :width => "100%", :height => "50%") %>
  <div class="card-block">
    <h4 class="card-title"> <%= photo.username %> </h4>
    <p class="card-text"> <%= photo.caption %> </p>
    <p class="card-text"> Like counts: <%= photo.likes_count %> </p>
  </div>
</div>